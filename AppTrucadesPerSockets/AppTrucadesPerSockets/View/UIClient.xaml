<UserControl x:Class="AppTrucadesPerSockets.View.UIClient"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AppTrucadesPerSockets.View"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal">
        <TextBlock x:Name="txbNom" Grid.Column="0" Text="NOM" Margin="5"/>
        <TextBlock Text="-" Grid.Column="1" Margin="2 0" VerticalAlignment="Center"/>
        <TextBlock x:Name="txbTelefon" Grid.Column="2" Text="TELEFON" Margin="5"/>
    </StackPanel>
</UserControl>
